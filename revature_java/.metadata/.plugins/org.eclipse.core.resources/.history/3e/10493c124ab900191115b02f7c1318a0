package example1;

public class Employee {
	int id;
	String name;
	int age;
	String city;
	
	public Employee() {
		// TODO Auto-generated constructor stub
	}
	

	
	
	public Employee(int id, String name, int age, String city) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
		this.city = city;
	}




	
}
