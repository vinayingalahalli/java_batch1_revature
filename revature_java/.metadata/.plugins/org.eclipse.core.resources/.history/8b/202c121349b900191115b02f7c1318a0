package example1;

public class Employee {
	int id;
	String name;
	int age;
	String city;
	
	public Employee() {
		// TODO Auto-generated constructor stub
	}
	

	
	
	public void printEmployee() {
		System.out.println("Id  = "+this.id);
		System.out.println("Name = "+this.name);
		System.out.println("Age = "+age);
		System.out.println("City = "+city);
	}
}
