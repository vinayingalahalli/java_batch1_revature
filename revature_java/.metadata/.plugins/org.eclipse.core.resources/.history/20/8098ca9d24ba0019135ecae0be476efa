package override;

public class MobileV1 {

	public void calling() {
		System.out.println("Calling feature of V1");
	}
	
	public void texting() {
		System.out.println("Text message feature from V1");
	}
	
	protected void displayMenu() {
		System.out.println("Menu in V1 displays in LIST style");
	}
}
