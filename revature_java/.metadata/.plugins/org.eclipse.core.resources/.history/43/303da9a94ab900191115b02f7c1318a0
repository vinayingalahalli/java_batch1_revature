package example1;

public class Employee {
	private int id;
	private String name;
	private int age;
	private String city;
	
	public Employee() {
		// TODO Auto-generated constructor stub
	}
	

	
	
	public Employee(int id, String name, int age, String city) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
		this.city = city;
	}


 public void setId(int id) {
	 this.id=id;
 }

	
}
