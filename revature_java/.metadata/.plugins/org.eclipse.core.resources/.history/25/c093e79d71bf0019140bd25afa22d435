package example1;

import java.util.LinkedList;
import java.util.List;

public class Demo1 {

	public static void main(String[] args) {
		
		List li=new LinkedList();
		li.add("dsfhds");
		li.add(23.44);
		li.add(99);
		li.add(true);
		li.add('c');
		li.add(null);
		System.out.println(li);
		
		
		
		List<Integer> li2=new LinkedList<>();
		li2.add(123);
		li2.add(123);
		li2.add(4555);
		li2.add(1233);
		li2.add(999);
		li2.add(null);
		li2.add(null);
		li2.add(23);
		li2.add(44);
		li2.add(123);
		System.out.println(li2);
		li2.add(2, 9);
		System.out.println(li2);
		System.out.println(li2.size());
		
		
		
		
		
		

	}

}
